{
    "configurations": [
        {
            "name": "STM32 F0",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/Inc",
                "${workspaceFolder}/Drivers_CMSIS/Include",
                "${workspaceFolder}/Drivers_CMSIS/Device_MemDefs/STM32F4xx/Include",
                "/usr/lib/arm-none-eabi/include",
                "/usr/lib/arm-none-eabi/include/machine",
                "/usr/lib/arm-none-eabi/include/nano",
                "/usr/lib/arm-none-eabi/include/sys"
            ],
            "defines": [
                "STM32F401xE"
            ],
            "compilerPath": "/usr/bin/arm-none-eabi-gcc",
            "compilerArgs": [
                "-mcpu=cortex-m4",
                "-mthumb"
            ],
            "cStandard": "c11",
            "cppStandard": "c++20",
            "intelliSenseMode": "linux-gcc-arm64",
            "browse": 
            {
                "path":
                [
                    "${workspaceFolder}",
                    "${workspaceFolder}/**",
                    "${workspaceFolder}/Inc",
                    "${workspaceFolder}/Drivers_CMSIS/Include",
                    "${workspaceFolder}/Drivers_CMSIS/Device_MemDefs/STM32F4xx/Include",
                    "/usr/lib/arm-none-eabi/include"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": "${workspaceFolder}/.vscode/browse.vc.db"
                
            }
        }
    ],
    "version": 4
}